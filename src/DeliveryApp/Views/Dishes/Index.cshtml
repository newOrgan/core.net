@model IEnumerable<DeliveryApp.Models.Dishes>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Список еды</title>
</head>
<body>
    <div>
      @foreach (var dishes in Model)
      {

          if (dishes.key == ViewBag.Dish && dishes.RestaurantID == ViewBag.Rest)
          {
              
                <p>@dishes.name @dishes.price</p>
                
                <a href="~/Order/Create/@dishes.id">Добавить</a>
          }

      }

    </div>
</body>
</html>